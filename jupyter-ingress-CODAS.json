{
  "kind": "Ingress",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "xxx",
    "namespace": "xxx",
    "annotations": {
      "kubernetes.io/ingress.class": "slate"
    },
    "labels": {
      "k8s-app": "privatejupyter",
      "instance": "xxx"
    }
  },
  "spec": {
    "tls": [
      {
        "host": "xxx",
        "secretName": "codas-tls"
      }
    ],
    "rules": [
      {
        "host": "xxx",
        "http": {
          "paths": [
            {
              "path": "/",
              "backend": {
                "serviceName": "xxx",
                "servicePort": 80
              }
            }
          ]
        }
      }
    ]
  }
}